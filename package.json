{
  "name": "sacred-traditions",
  "description": "Home of 'Ocean 2.0 Interfaith Reader' and lots of other fun projects...",
  "version": "0.1.10",
  "scripts": {
    "dev": "sapper dev -p 1844 -o",
    "export": "NODE_ENV=production sapper export",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "deploy": "NODE_ENV=production sapper export && push-dir --dir=__sapper__/export --branch=gh-pages --cleanup --verbose",
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "build": "npm run build:tailwind && sapper build --legacy"
  },
  "repository": "git@github.com:chadananda/st-dev.git",
  "keywords": [
    "religion",
    "interfaith",
    "sacred traditions",
    "ocean",
    "ocean reader",
    "ocean 2.0",
    "dawn-breakers",
    "dawn-breakers challenge"
  ],
  "homepage": "https://sacred-traditions.org",
